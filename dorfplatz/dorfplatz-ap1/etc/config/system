config system
        option hostname 'dorfplatz-ap1'
        option zonename 'Europe/Berlin'
        option timezone 'CET-1CEST,M3.5.0,M10.5.0/3'
        option ttylogin '0'
        option log_size '64'
        option urandom_seed '0'

config timeserver 'ntp'
        option enabled '1'
        option enable_server '0'
        list server '0.openwrt.pool.ntp.org'
        list server '1.openwrt.pool.ntp.org'
        list server '2.openwrt.pool.ntp.org'
        list server '3.openwrt.pool.ntp.org'

config rssid 'rssid_wlan0'
	option dev 'wlan0'
	option refresh '200000'
	option threshold '1'

config led 'led_rssilow'
	option trigger 'rssi'
	option iface 'rssid_wlan0'
	option minq '1'
	option maxq '100'
	option offset '0'
	option factor '1'
	option sysfs 'green:rssilow'

config led 'led_rssimediumlow'
	option trigger 'rssi'
	option iface 'rssid_wlan0'
	option minq '21'
	option maxq '100'
	option offset '0'
	option factor '1'
	option sysfs 'green:rssimediumlow'

config led 'led_rssimedium'
	option trigger 'rssi'
	option iface 'rssid_wlan0'
	option minq '41'
	option maxq '100'
	option offset '0'
	option factor '1'
	option sysfs 'green:rssimedium'

config led 'led_rssimediumhigh'
	option trigger 'rssi'
	option iface 'rssid_wlan0'
	option minq '61'
	option maxq '100'
	option offset '0'
	option factor '1'
	option sysfs 'green:rssimediumhigh'

config led 'led_rssihigh'
	option trigger 'rssi'
	option iface 'rssid_wlan0'
	option minq '81'
	option maxq '100'
	option offset '0'
	option factor '1'
	option sysfs 'green:rssihigh'

