config wifi-device 'radio0'
        option type 'mac80211'
        option path 'pci0000:00/0000:00:00.0'
        option htmode 'VHT20'
        option country 'DE'
        option channel '36'
        option hwmode '11a'
        option legacy_rates '0'

config wifi-iface 'radio0_ffdhcp'
        option network 'dhcp'
        option device 'radio0'
        option ifname 'wlan5-ff'
        option mode 'ap'
        option ssid 'berlin.freifunk.net'
        option encryption 'none'
        option isolate '1'

config wifi-iface 'radio0_ffdhcp_owe'
        option network 'dhcp'
        option device 'radio0'
        option ifname 'wlan5-ffowe'
        option mode 'ap'
        option ssid 'berlin.freifunk.net Encrypted'
        option encryption 'owe'
        option isolate '1'

config wifi-device 'radio1'
        option type 'mac80211'
	option path 'platform/ahb/18100000.wmac'
        option htmode 'HT20'
        option country 'DE'
        option channel '13'
        option hwmode '11g'
        option legacy_rates '0'

config wifi-iface 'radio1_ffdhcp'
        option network 'dhcp'
        option device 'radio1'
        option ifname 'wlan2-ff'
        option mode 'ap'
        option ssid 'berlin.freifunk.net'
        option encryption 'none'
        option isolate '1'

config wifi-iface 'radio1_ffdhcp_owe'
        option network 'dhcp'
        option device 'radio1'
        option ifname 'wlan2-ffowe'
        option mode 'ap'
        option ssid 'berlin.freifunk.net Encrypted'
        option encryption 'owe'
        option isolate '1'
