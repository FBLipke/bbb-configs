config system
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option cronloglevel '10'
	option zonename 'Europe/Berlin'
	option timezone 'CET-1CEST,M3.5.0,M10.5.0/3'
	option hostname 'sama-core'
	option location 'Samariterkirche'
	option latitude '52.518044835152963'
	option longitude '13.466172516345976'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server '0.openwrt.pool.ntp.org'
	list server '1.openwrt.pool.ntp.org'
	list server '2.openwrt.pool.ntp.org'
	list server '3.openwrt.pool.ntp.org'
	option use_dhcp '0'

config led 'led_wlan'
	option name 'WLAN'
	option trigger 'phy0tpt'
	option sysfs 'green:wlan'

config led 'led_wan'
	option name 'WAN'
	option trigger 'netdev'
	option mode 'link tx rx'
	option dev 'eth1'
	option sysfs 'green:wan'

config led 'led_lan'
	option name 'LAN'
	option trigger 'switch0'
	option port_mask '0x1e'
	option sysfs 'green:lan'
