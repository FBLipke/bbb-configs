config wifi-device 'radio1'
	option type 'mac80211'
	option country 'DE'
	option hwmode '11g'
	option path 'platform/soc/a000000.wifi'
	option htmode 'HT20'
	option channel '13'

config wifi-iface 'wifi2_ff'
	option ifname 'wifi2-ff'
	option network 'dhcp'
	option encryption 'none'
	option device 'radio1'
	option mode 'ap'
	option ssid 'berlin.freifunk.net'

config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11a'
	option path 'platform/soc/a800000.wifi'
	option htmode 'VHT40'
	option channel '36'
	option country 'DE'

config wifi-iface 'wifi5_ff'
	option ifname 'wifi5-ff'
	option network 'dhcp'
	option encryption 'none'
	option device 'radio0'
	option mode 'ap'
	option ssid 'berlin.freifunk.net'
