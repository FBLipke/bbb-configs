#jinja2: trim_blocks: "true", lstrip_blocks: "true"
{% for device in airos_dfs_reset|default([]) %}
config device '{{ device['name'] | replace('-','_') }}'
	option target '{{ device['target'] }}'
	option username '{{ device['username'] }}'
	option password '{{ device['password'] }}'
	option daytime_limit '{{ device['daytime_limit'] }}'
{% endfor %}
